{
  "name": "proxipoll",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run backend & bun run frontend",
    "frontend": "vite --open",
    "backend": "bun backend/index.ts",
    "createDB": "psql -U proxipoll -d proxipoll -a -f ./database/tables/mainTables.sql",
    "dropDB": "psql -U proxipoll -d proxipoll -a -f ./database/stored_procedures/dropTables.sql",
    "kill": "kill -9 $(lsof -t -i tcp:5173); kill -9 $(lsof -t -i tcp:7500)",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
