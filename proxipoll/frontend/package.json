{
  "name": "proxipoll",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "development": "bun run kill && bun run database && (bun run backend & bun run frontend)",
    "frontend": "vite --open",
    "backend": "bun run backend/index.ts",
    "database": "psql postgresql://proxipoll:proximity@localhost -f ./database/schemas/account_schema.sql -f ./database/schemas/poll_schema.sql",
    "drop-database": "psql postgresql://proxipoll:proximity@localhost -f ./database/stored_procedures/drop_schemas.sql",
    "kill": "kill -9 $(lsof -t -i :5173) || true && kill -9 $(lsof -t -i :7500) || true",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.14",
    "@mui/material": "^5.14.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
